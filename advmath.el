;;; Example of possible YOLOLISP
;;; Vector/Matrix library operations

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;;; Vector
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

(vset a '(7 8 9)) "a1=7 a2=8 a3=9"

(vsmul a 3) ; "a1*=3 a2*=3 a3*=3"

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;;; Matrix
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

(mset b '((0 1)
          (0 0)))
;; "b1_1=0 b1_2=1 b2_1=0 b2_2=0"

(mset c '((0 0)
          (1 0)))
;; "c1_1=0 c1_2=0 c2_1=1 c2_2=0"

(mset d (mmmul b c))
;; "d1_1=b1_1*c1_1+b1_2*c2_1"
;; "d1_2=b1_1*c1_2+b1_2*c2_2"
;; "d2_1=b2_1*c1_1+b2_2*c2_1"
;; "d2_2=b2_1*c1_2+b2_2*c2_2"
;; ;; equivalent to
;; ((1 0)
;;  (0 0))

;; YOLOL variables for compiled Matrix stuff:
;; b1_1 0
;; b1_2 1
;; b2_1 0
;; b2_2 0
;; c1_1 0
;; c1_2 0
;; c2_1 1
;; c2_2 0
;; d1_1 1
;; d1_2 0
;; d2_1 0
;; d2_2 0
